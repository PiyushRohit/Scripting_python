:vpi_module "C:\iverilog\lib\ivl\v2005_math.vpi";
:vpi_module "C:\iverilog\lib\ivl\va_math.vpi";
S_000001e7787884c0 .scope module, "bcd_counter_tb" "bcd_counter_tb" 2 1;
 .timescale 0 0;
v000001e7786daf50_0 .var "clk", 0 0;
v000001e7786daff0_0 .net "count", 3 0, v000001e778788650_0;  1 drivers
v000001e7786db090_0 .var "down", 0 0;
v000001e7786db4b0_0 .var "reset", 0 0;
v000001e7786db550_0 .var "up", 0 0;
S_000001e7786dbc10 .scope module, "uut" "bcd_counter" 2 6, 3 1 0, S_000001e7787884c0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "count";
    .port_info 1 /INPUT 1 "up";
    .port_info 2 /INPUT 1 "down";
    .port_info 3 /INPUT 1 "clk";
    .port_info 4 /INPUT 1 "reset";
v000001e7787634b0_0 .net "clk", 0 0, v000001e7786daf50_0;  1 drivers
v000001e778788650_0 .var "count", 3 0;
v000001e7786dbda0_0 .net "down", 0 0, v000001e7786db090_0;  1 drivers
v000001e7786dae10_0 .net "reset", 0 0, v000001e7786db4b0_0;  1 drivers
v000001e7786daeb0_0 .net "up", 0 0, v000001e7786db550_0;  1 drivers
E_000001e778786d30 .event posedge, v000001e7787634b0_0;
    .scope S_000001e7786dbc10;
T_0 ;
    %wait E_000001e778786d30;
    %load/vec4 v000001e7786dae10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v000001e778788650_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000001e7786daeb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v000001e778788650_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v000001e778788650_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v000001e778788650_0;
    %addi 1, 0, 4;
    %assign/vec4 v000001e778788650_0, 0;
T_0.5 ;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v000001e7786dbda0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %load/vec4 v000001e778788650_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.8, 4;
    %pushi/vec4 9, 0, 4;
    %assign/vec4 v000001e778788650_0, 0;
    %jmp T_0.9;
T_0.8 ;
    %load/vec4 v000001e778788650_0;
    %subi 1, 0, 4;
    %assign/vec4 v000001e778788650_0, 0;
T_0.9 ;
T_0.6 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_000001e7787884c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786daf50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db550_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db090_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db4b0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_000001e7787884c0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v000001e7786daf50_0;
    %inv;
    %store/vec4 v000001e7786daf50_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_000001e7787884c0;
T_3 ;
    %vpi_call 2 13 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_000001e7787884c0 {0 0 0};
    %vpi_call 2 17 "$monitor", "Time = %0t | clk = %b | reset = %b | up = %b | down = %b | count = %d", $time, v000001e7786daf50_0, v000001e7786db4b0_0, v000001e7786db550_0, v000001e7786db090_0, v000001e7786daff0_0 {0 0 0};
    %vpi_call 2 21 "$display", "----- Applying Reset -----" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e7786db4b0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db4b0_0, 0, 1;
    %vpi_call 2 24 "$display", "----- Begin Up Counting -----" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e7786db550_0, 0, 1;
    %pushi/vec4 12, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %vpi_call 2 28 "$display", "----- Begin Down Counting -----" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db550_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e7786db090_0, 0, 1;
    %pushi/vec4 12, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call 2 33 "$display", "----- Test up = down = 1 (conflicting input) -----" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e7786db090_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e7786db550_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db090_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e7786db550_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 37 "$display", "----- Simulation Complete -----" {0 0 0};
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_3;
